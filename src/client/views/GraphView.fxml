<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.GraphViewController">
   <bottom>
      <AreaChart fx:id="temperatureChart" prefHeight="275.0" prefWidth="760.0" title="Temperatur" BorderPane.alignment="CENTER">
        <xAxis>
          <NumberAxis label="Tid" side="BOTTOM" upperBound="95.0" />
        </xAxis>
        <yAxis>
          <NumberAxis label="C°" side="LEFT" upperBound="45.0" />
        </yAxis>
         <BorderPane.margin>
            <Insets left="20.0" right="20.0" />
         </BorderPane.margin>
      </AreaChart>
   </bottom>
   <center>
      <AreaChart fx:id="valueChart" prefHeight="275.0" prefWidth="760.0" title="Värde" BorderPane.alignment="CENTER">
        <xAxis>
          <NumberAxis label="Tid" side="BOTTOM" upperBound="95.0" />
        </xAxis>
        <yAxis>
          <NumberAxis label="\%" side="LEFT" upperBound="90.0" />
        </yAxis>
         <BorderPane.margin>
            <Insets left="20.0" right="20.0" top="20.0" />
         </BorderPane.margin>
      </AreaChart>
   </center>
   <top>
      <Label fx:id="plantAliasLabel" BorderPane.alignment="CENTER" />
   </top>
</BorderPane>
